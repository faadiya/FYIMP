#include <stdio.h>
#include <math.h>
int main() 
{   float a,b,c,r1,r2,d;
     
    printf("Ente a,b,c:\n");
    scanf("%f %f %f",&a,&b,&c);
    
    d=(b*b)-(4*a*c);
   // printf("%")
    r1=(-b+sqrt(d))/(2*a);
    r2=(-b-sqrt(d))/(2*a);
    printf("Root 1 = %f\n",r1);
     printf("Root 2 = %f\n",r2);
    return 0;
}
